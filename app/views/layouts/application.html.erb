<!DOCTYPE html>
<html>
<head>
  <title>ProcessTracking</title>
  <link rel="stylesheet" href="stylesheets/blueprint-css/screen.css" type="text/css" media="screen, projection">
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag 'jquery' %>
  <%= csrf_meta_tag %>
</head>
<body>

<% [:notice, :error, :warning].map do |flash_type| %>
  <%= content_tag(:div, flash[flash_type], :class => 'flash', :id => "message_#{  flash_type}") %>
<% end %>

<%= yield %>
<%= yield :javascript %>

</body>
</html>



